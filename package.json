{
  "name": "teste-tecnico-igma",
  "version": "1.0.0",
  "description": "API para cadastro de clientes",
  "main": "index.js",
  "scripts": {
    "test": "jest -- --runInBand",
    "test:load-env": "dotenv -e .env.test.local",
    "test:compose": "docker-compose -f docker-compose.test.yml --env-file .env.test",
    "test:postgres:up": "npm run test:compose -- up -d igma-postgres-test",
    "test:docker:down": "npm run test:compose -- down",
    "test:docker": "npm run test:compose -- up",
    "test:local": "npm run test:load-env -- npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akiraTatesawa/teste-tecnico-igma.git"
  },
  "author": "Arthur Akira Tatesawa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/akiraTatesawa/teste-tecnico-igma/issues"
  },
  "homepage": "https://github.com/akiraTatesawa/teste-tecnico-igma#readme",
  "dependencies": {
    "@ngneat/falso": "^6.2.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "node": "^19.1.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.18",
    "@swc/jest": "^0.2.23",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "supertest": "^6.3.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^2.0.2",
    "tsc-alias": "^1.7.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3",
    "typescript-transform-paths": "^3.4.4"
  }
}
